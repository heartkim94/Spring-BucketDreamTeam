-- 여기에 추가

-- #Community
-- table Notice
create table Notice(
    articleNum int primary key auto_increment,
    id varchar(300) not null,
    title varchar(500) not null,
    content text not null,
    depth int not null,
    hit int not null,
    groupId int not null,
    writeDate date not null,
    fileStatus int(1) not null);